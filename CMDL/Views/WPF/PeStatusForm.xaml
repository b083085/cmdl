<Window x:Class="CMDL.PeStatusForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PHYSICAL EXAMINATION STATUS" Height="150" Width="250" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Margin="5" HorizontalAlignment="Center" Content="Done" Width="200" Name="btDone"/>
        <Button Grid.Row="1" Margin="5" HorizontalAlignment="Center" Content="Evaluation Pending" Width="200" Name="btEvaluationPending"/>
    </Grid>
</Window>
