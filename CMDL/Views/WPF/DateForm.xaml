<Window x:Class="CMDL.DateForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DATE FORM" Height="130" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        
        <DatePicker Grid.Row="0" Margin="5" Name="dpDateSearch"/>
        <Button Grid.Row="1" Content="OK" Margin="5" Name="btOK"/>
    </Grid>
</Window>
