<UserControl x:Class="CMDL.NeuroV2Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:conv="clr-namespace:CMDL.Converters"
             Background="#fff"
             mc:Ignorable="d" 
             d:DesignHeight="450" 
             d:DesignWidth="800"
             Width="775"
             Height="1680"
             BorderBrush="LightGray"
             BorderThickness="1"
             FontFamily="Arial">
    <UserControl.Resources>
        <Style TargetType="{x:Type TextBox}">
            <Setter Property="Background" Value="LightYellow"/>
            <Setter Property="Margin" Value="2"/>
        </Style>
        <Style TargetType="{x:Type ComboBox}">
            <Setter Property="Margin" Value="2"/>
            <Setter Property="Background" Value="LightYellow"/>
        </Style>
        <conv:NegateReadOnlyVisibilityConverter x:Key="negateReadOnlyVisibilityConverter"/>
        <conv:ReadOnlyVisibilityConverter x:Key="readOnlyVisibilityConverter"/>
        <conv:ReadOnlyConverter x:Key="readOnlyConverter"/>
        <conv:SaveRecordContentConverter x:Key="saveRecordContentConverter"/>
    </UserControl.Resources>
    <Grid Margin="20" DataContext="{Binding Path=Neuro}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="180"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="250"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Label Content="Name" FontWeight="Bold"/>
        <Label Content="Home Address" FontWeight="Bold" Grid.Row="1"/>
        <Label Content="Occupation" FontWeight="Bold" Grid.Row="2"/>
        <Label Content="Educational Attainment" FontWeight="Bold" Grid.Row="3"/>
        <Label Content="Job Experience" FontWeight="Bold" Grid.Row="4"/>
        <Label Content="Purpose" FontWeight="Bold" Grid.Row="5"/>
        <Label Content="Eligibility" FontWeight="Bold" Grid.Row="6"/>
        <Label Content="Refering Agency" FontWeight="Bold" Grid.Row="7"/>

        <Label Content=":" Grid.Column="1"/>
        <Label Content=":" Grid.Column="1" Grid.Row="1"/>
        <Label Content=":" Grid.Column="1" Grid.Row="2"/>
        <Label Content=":" Grid.Column="1" Grid.Row="3"/>
        <Label Content=":" Grid.Column="1" Grid.Row="4"/>
        <Label Content=":" Grid.Column="1" Grid.Row="5"/>
        <Label Content=":" Grid.Column="1" Grid.Row="6"/>
        <Label Content=":" Grid.Column="1" Grid.Row="7"/>

        <TextBox Grid.Column="2" 
                 IsReadOnly="True" 
                 Name="tbName" 
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                 Text="{Binding Path=Client.FullName}"/>
        <TextBox Grid.Column="2" 
                 Grid.Row="1" 
                 IsReadOnly="True" 
                 Name="tbHomeAddress" 
                 Text="{Binding Path=Client.HomeAddress}" 
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"/>
        <TextBox Grid.Column="2" 
                 Grid.Row="2" 
                 MaxLength="1000"
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}" 
                 Name="tbOccupation" 
                 CharacterCasing="Upper"
                 Text="{Binding Path=Occupation,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
        <TextBox Grid.Column="2" 
                 Grid.Row="3" 
                 MaxLength="1000"
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}" 
                 Name="tbEducationalAttainment" 
                 CharacterCasing="Upper"
                 Text="{Binding Path=EducationalAttainment,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
        <TextBox Grid.Column="2"
                 Grid.Row="4"
                 MaxLength="1000"
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                 Name="tbJobExperience"
                 CharacterCasing="Upper"
                 Text="{Binding Path=JobExperience,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
        <TextBox Grid.Column="2"
                 Grid.Row="5"
                 IsReadOnly="True" 
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                 Name="tbPurpose"
                 Text="{Binding Path=Client.Purpose}"/>
        <TextBox Grid.Column="2"
                 Grid.Row="6"
                 MaxLength="1000"
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                 Name="tbEligibility"
                 CharacterCasing="Upper"
                 Text="{Binding Path=Eligibility,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
        <TextBox Grid.Column="2" 
                 Grid.Row="7" 
                 IsReadOnly="True" 
                 Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}" 
                 Name="tbReferringAgency" 
                 Text="{Binding Path=Client.RequestingParty}"/>

        <Label Grid.Column="2"
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"
               Content="{Binding Path=Client.FullName}"/>
        <Label Grid.Column="2"
               Grid.Row="1"
                   Content="{Binding Path=Client.HomeAddress}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
        <Label Grid.Column="2"
               Grid.Row="2"
                   Content="{Binding Path=Occupation}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
        <Label Grid.Column="2"
               Grid.Row="3"
                   Content="{Binding Path=EducationalAttainment}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
        <Label Grid.Column="2"
               Grid.Row="4"
                   Content="{Binding Path=JobExperience}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
        <Label Grid.Column="2"
               Grid.Row="5"
                   Content="{Binding Path=Client.Purpose}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
        <Label Grid.Column="2"
               Grid.Row="6"
                   Content="{Binding Path=Eligibility}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
        <Label Grid.Column="2"
               Grid.Row="7"
                   Content="{Binding Path=Client.RequestingParty}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>

        <Border Grid.Column="3" Grid.RowSpan="7" BorderBrush="{StaticResource btnBackgroundStyle}" BorderThickness="2" Margin="2">
            <Image Source="{Binding Path=Client.Photo}"/>
        </Border>
        <Label Grid.Column="3" Grid.Row="7" HorizontalContentAlignment="Center" Foreground="Red" FontSize="16" FontWeight="Bold" Content="{Binding Path=Client.ControlNo}"/>

        <GroupBox Header="Test Administered" Margin="0,10,0,0" FontWeight="Bold" Grid.ColumnSpan="4" Grid.Row="8">
            <Grid Margin="20">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Label Content="IQ Test"/>
                <Label Content="Retake" Grid.Row="1"/>
                <Label Content="Personality Test" Grid.Row="2"/>
                <Label Content="Others" Grid.Row="3"/>

                <Label Content=":" Grid.Column="1"/>
                <Label Content=":" Grid.Column="1" Grid.Row="1"/>
                <Label Content=":" Grid.Column="1" Grid.Row="2"/>
                <Label Content=":" Grid.Column="1" Grid.Row="3"/>

                <ComboBox Grid.Column="2" 
                          IsEditable="True" 
                          Name="cbIQTest" 
                          Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                          Text="{Binding Path=IQTest,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                <Label Grid.Column="2"
                   Content="{Binding Path=IQTest}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
                <ComboBox Grid.Column="2" 
                          Grid.Row="1" 
                          IsEditable="True" 
                          Name="cbRetake" 
                          Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                          Text="{Binding Path=Retake,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                <Label Grid.Column="2"
                       Grid.Row="1"
                   Content="{Binding Path=Retake}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
                <ComboBox Grid.Column="2" 
                          Grid.Row="2" 
                          IsEditable="True" 
                          Name="cbPersonalityTest" 
                          Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                          Text="{Binding Path=PersonalityTest,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                <Label Grid.Column="2"
                       Grid.Row="2"
                   Content="{Binding Path=PersonalityTest}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
                <ComboBox Grid.Column="2" 
                          Grid.Row="3" 
                          IsEditable="True" 
                          Name="cbOthers" 
                          Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource negateReadOnlyVisibilityConverter}}"
                          Text="{Binding Path=Others,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                <Label Grid.Column="2"
                       Grid.Row="3"
                   Content="{Binding Path=Others}" 
                   Visibility="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyVisibilityConverter}}"/>
            </Grid>
        </GroupBox>
        <Label Grid.Row="9" 
               Background="{StaticResource btnBackgroundStyle}"
               Foreground="#fff"
               FontWeight="Bold"
               FontSize="15"
               Grid.ColumnSpan="4"
               Content="NEURO PSYCHOLOGICAL TEST REPORT"
               HorizontalContentAlignment="Center"/>
        <Grid Grid.Row="10" Grid.ColumnSpan="4">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Label Content="A. INTELLIGENCE TEST" FontWeight="Bold"/>
            <Label Content="" Grid.Row="1"/>
            <Label Content="Below Average" Grid.Column="1" HorizontalAlignment="Center"/>
            <Label Content="Lower Average" Grid.Column="2" HorizontalAlignment="Center"/>
            <Label Content="Average" Grid.Column="3" HorizontalAlignment="Center"/>
            <Label Content="Above Average" Grid.Column="4" HorizontalAlignment="Center"/>
            <Label Content="Superior" Grid.Column="5" HorizontalAlignment="Center"/>

            <RadioButton Grid.Column="1" Grid.Row="1" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Intelligence" Name="rb_IntTest_BelowAverage"/>
            <RadioButton Grid.Column="2" Grid.Row="1" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Intelligence" Name="rb_IntTest_LowerAverage"/>
            <RadioButton Grid.Column="3" Grid.Row="1" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Intelligence" Name="rb_IntTest_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="1" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Intelligence" Name="rb_IntTest_AboveAverage"/>
            <RadioButton Grid.Column="5" Grid.Row="1" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Intelligence" Name="rb_IntTest_Superior"/>

            <Rectangle Grid.Row="2" Grid.ColumnSpan="6" Height="1" Fill="Black" Stroke="Black" Margin="0,5,0,5"/>

            <Label Content="B. PERSONALITY TEST" Grid.Row="3" FontWeight="Bold" Visibility="Collapsed"/>
            <Label Content="Very Low" Grid.Column="1" Grid.Row="3" HorizontalAlignment="Center"/>
            <Label Content="Low" Grid.Column="2" Grid.Row="3" HorizontalAlignment="Center"/>
            <Label Content="Average" Grid.Column="3" Grid.Row="3" HorizontalAlignment="Center"/>
            <Label Content="High" Grid.Column="4" Grid.Row="3" HorizontalAlignment="Center"/>
            <Label Content="Very High" Grid.Column="5" Grid.Row="3" HorizontalAlignment="Center"/>

            <Label Content="Neuroticism" Grid.Row="4" Visibility="Collapsed"/>
            <RadioButton Grid.Column="1" Grid.Row="4" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Neuroticism" Name="rb_Neuroticism_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="4" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Neuroticism" Name="rb_Neuroticism_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="4" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Neuroticism" Name="rb_Neuroticism_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="4" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Neuroticism" Name="rb_Neuroticism_High"/>
            <RadioButton Grid.Column="5" Grid.Row="4" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Neuroticism" Name="rb_Neuroticism_VeryHigh"/>

            <Label Content="Extraversion" Grid.Row="5" Visibility="Collapsed"/>
            <RadioButton Grid.Column="1" Grid.Row="5" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Extraversion" Name="rb_Extraversion_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="5" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Extraversion" Name="rb_Extraversion_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="5" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Extraversion" Name="rb_Extraversion_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="5" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Extraversion" Name="rb_Extraversion_High"/>
            <RadioButton Grid.Column="5" Grid.Row="5" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Extraversion" Name="rb_Extraversion_VeryHigh"/>

            <Label Content="Openness" Grid.Row="6" Visibility="Collapsed"/>
            <RadioButton Grid.Column="1" Grid.Row="6" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Openness" Name="rb_Openness_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="6" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Openness" Name="rb_Openness_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="6" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Openness" Name="rb_Openness_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="6" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Openness" Name="rb_Openness_High"/>
            <RadioButton Grid.Column="5" Grid.Row="6" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Openness" Name="rb_Openness_VeryHigh"/>

            <Label Content="Agreeableness" Grid.Row="7" Visibility="Collapsed"/>
            <RadioButton Grid.Column="1" Grid.Row="7" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Agreeableness" Name="rb_Agreeableness_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="7" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Agreeableness" Name="rb_Agreeableness_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="7" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Agreeableness" Name="rb_Agreeableness_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="7" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Agreeableness" Name="rb_Agreeableness_High"/>
            <RadioButton Grid.Column="5" Grid.Row="7" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Agreeableness" Name="rb_Agreeableness_VeryHigh"/>

            <Label Content="Conscientiousness" Grid.Row="8" Visibility="Collapsed"/>
            <RadioButton Grid.Column="1" Grid.Row="8" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Conscientiousness" Name="rb_Conscientiousness_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="8" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Conscientiousness" Name="rb_Conscientiousness_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="8" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Conscientiousness" Name="rb_Conscientiousness_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="8" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Conscientiousness" Name="rb_Conscientiousness_High"/>
            <RadioButton Grid.Column="5" Grid.Row="8" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="Conscientiousness" Name="rb_Conscientiousness_VeryHigh"/>

            <Label Content="B. EMOTIONAL STABILITY" Grid.Row="3" FontWeight="Bold"/>
            <Label Content="1. Coping with stress" Grid.Row="9"/>
            <RadioButton Grid.Column="1" Grid.Row="9" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B1" Name="rb_B1_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="9" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B1" Name="rb_B1_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="9" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B1" Name="rb_B1_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="9" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B1" Name="rb_B1_High"/>
            <RadioButton Grid.Column="5" Grid.Row="9" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B1" Name="rb_B1_VeryHigh"/>

            <Label Content="2. Control of aggressive/hostile impulses" Grid.Row="10"/>
            <RadioButton Grid.Column="1" Grid.Row="10" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B2" Name="rb_B2_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="10" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B2" Name="rb_B2_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="10" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B2" Name="rb_B2_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="10" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B2" Name="rb_B2_High"/>
            <RadioButton Grid.Column="5" Grid.Row="10" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="B2" Name="rb_B2_VeryHigh"/>

            <Label Content="C. SOCIAL ADAPTABILITY" Grid.Row="11" FontWeight="Bold"/>
            <Label Content="1. With people in general" Grid.Row="12"/>
            <RadioButton Grid.Column="1" Grid.Row="12" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="C1" Name="rb_C1_VeryLow"/>
            <RadioButton Grid.Column="2" Grid.Row="12" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="C1" Name="rb_C1_Low"/>
            <RadioButton Grid.Column="3" Grid.Row="12" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="C1" Name="rb_C1_Average"/>
            <RadioButton Grid.Column="4" Grid.Row="12" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="C1" Name="rb_C1_High"/>
            <RadioButton Grid.Column="5" Grid.Row="12" IsEnabled="{Binding Path=IsReadOnly,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged,Converter={StaticResource readOnlyConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" GroupName="C1" Name="rb_C1_VeryHigh"/>

